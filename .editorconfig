# editorconfig.org
# make sure that the appropriate plugin for the IDEis also enabled.
# without the pluginthe editor config will not be able to enforce the config

root = true

[*]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false
